    For i = 7 To 8
        For j = 0 To 4
            Delay 500
            Call Plugin.Bkgnd.LeftDown(Hwnd, Bag_Grid(i, j)(0), Bag_Grid(i, j)(1))
            TracePrint Bag_Grid(i, j)(0) & "|" & Bag_Grid(i, j)(1)
            Delay 500
            Call Plugin.Bkgnd.LeftUp(Hwnd, BagC(6)(0), BagC(6)(1))
            Delay 200
            Call Plugin.Bkgnd.LeftClick(Hwnd, PopU(0)(0), PopU(0)(1))
            
        Next
    Next
End Sub

